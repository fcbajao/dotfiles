[alias]
  c = commit -m
  ca = commit -a -m
  a = add
  di = diff
  dic = diff --cached
  up = pull --rebase
  st = status
  out = log origin..HEAD
[user]
  name = Erick Bajao
  email = fcbajao@gmail.com
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[core]
  excludesfile = ~/.gitignore_global
  editor = nvim
[credential]
  helper = osxkeychain
